/* 水平间距 */
.my-search-container {
  height: 50px;
  line-height: 50px;
  text-align: center;
  display: flex;
  align-items: center;
  padding: 0 10px;
}
.my-search-container .my-search-box {
  background-color: #fff;
  width: 100%;
  height: 36px;
  margin: 0 auto;
  line-height: 36px;
}
.my-search-container .my-search-box text {
  margin-left: 5px;
  font-size: 15px;
}

